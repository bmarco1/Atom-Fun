SELECT type, SUM(minutes) FROM Weight_Training GROUP BY type;

SELECT type, SUM(minutes) FROM Weight_Training WHERE minutes > 80
GROUP BY type;


SELECT type, SUM(ounces_of_water) FROM Weight_Training GROUP BY type;
